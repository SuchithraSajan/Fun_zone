{
    "builds":[
        {
            "src":"FUNZONE/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"30mb",
                "runtime":"python3.13"

            }
        }
    ],
    "routes":[
        {
            "src" : "/(,*)",
            "dest":"FUNZONE/wsgi.py"
        }
    ]
}